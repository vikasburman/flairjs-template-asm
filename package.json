{
    "name": "flairjs-template-asm",
    "title": "FlairJS Assembly Project Template",
    "description": "Boilerplate project structure for creating a flairjs assembly",
    "copyright": "(c) 2017-2019 Vikas Burman",
    "version": "0.9.6",
    "author": "Vikas Burman <vikasburman@gmail.com>",
    "keywords": [
        "javascript",
        "oops",
        "javascript-framework",
        "isomorphic-javascript"
    ],
    "engines": {
        "node": "12"
    },
    "license": "MIT",
    "link": "https://flairjs.com",
    "homepage": "https://flairjs.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/vikasburman/flairjs-template-asm.git"
    },
    "scripts": {
        "build": "gulp build --options ./config/build.json",
        "build-full": "gulp build --full --options ./config/build.json",
        "build-quick": "gulp build --quick --options ./config/build.json",
        "test": "gulp test --TEST --options ./config/test.json",
        "package": "gulp package --options ./config/pack.json && npm pack ./publish/package/npm",
        "start": "http-server ./ -p8088 &",
        "stop": "pkill -f http-server"
    },
    "devDependencies": {
        "clean-css": "^4.2.1",
        "copy-dir": "^1.1.0",
        "del": "^5.0.0",
        "eslint": "^6.0.1",
        "fast-glob": "^3.0.4",
        "flairjs": "^0.9.37",
        "fs-extra": "^8.1.0",
        "gulp": "^4.0.2",
        "html-minifier": "^4.0.0",
        "htmllint": "^0.8.0",
        "http-server": "^0.11.1",
        "jasmine": "^3.4.0",
        "minimist": "^1.2.0",
        "open": "^6.4.0",
        "recursive-readdir-sync": "^1.0.6",
        "stylelint": "^10.1.0",
        "uglify-es": "^3.3.9"
    },
    "dependencies": {}
}